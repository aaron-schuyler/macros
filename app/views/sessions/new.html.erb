<% flash.each do |type, msg| %>
  <div class="alert alert-danger">
    <%= msg %>
  </div>
<% end %>
<%= form_tag '/login' do %>
  <%= text_field_tag :username %>
  <%= password_field_tag :password %>
  <%= submit_tag "Login" %>
<% end %>

<%= link_to 'Log in with Google!', '/auth/google_oauth2' %>
